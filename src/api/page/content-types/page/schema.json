{
  "kind": "collectionType",
  "collectionName": "pages",
  "info": {
    "singularName": "page",
    "pluralName": "pages",
    "displayName": "page",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "pages"
    },
    "blocks": {
      "type": "dynamiczone",
      "components": [
        "blocks.about",
        "blocks.note",
        "blocks.section-with-list",
        "blocks.guarantee",
        "blocks.price",
        "blocks.result",
        "blocks.what-need",
        "blocks.benefits"
      ]
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "shared.seo"
    }
  }
}
